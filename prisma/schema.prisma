// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
    relationMode = "prisma"
}


model Task {
    id Int @id @default(autoincrement())
    discipline String
    teatcher_name String @db.Text()
    question String @db.VarChar(255)
    response String 
    post_id Int
    student_name String
}

model Teatcher {
    teatcher_id Int @id @default(autoincrement())
    name String
}

model Student {
    id Int @id @default(autoincrement())
    name String
    period String
}